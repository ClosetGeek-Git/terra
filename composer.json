{
    "name": "closetgeek-git/terra",
    "description": "High-level Janus Admin Framework over ZeroMQ with ReactPHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ClosetGeek",
            "email": "info@closetgeek.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "react/zmq": "^0.4.0",
        "react/event-loop": "^1.2",
        "react/http": "^1.9",
        "react/socket": "^1.12",
        "monolog/monolog": "^2.0",
        "ext-zmq": "*"
    },
    "suggest": {
        "closetgeek-git/reactphp-zmq": "Preferred ZMQ implementation with enhanced features",
        "friends-of-reactphp/zmq": "Alternative ZMQ implementation"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Terra\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Terra\\Tests\\": "tests/"
        }
    }
}
